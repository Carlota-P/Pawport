<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FormEditTextCompact">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:textColorHint">#666666</item>
        <item name="android:textColor">#666666</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
    </style>

    <style name="BottomNavStyle" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemIconSize">40dp</item>
    </style>

    <style name="CampoPerfilAjustado">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColorHint">#666666</item>
        <item name="android:backgroundTint">#66BC99</item>
        <item name="android:inputType">textPersonName</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="CustomDialog" parent="Theme.MaterialComponents.Light.Dialog.Alert">
        <item name="android:background">@android:color/white</item>
    </style>

</resources>